void setup() {
  Serial.begin(115200); //串口通讯
  u8g2.setBusClock(9000000);   //设置时钟
  u8g2.begin();//初始化
  u8g2.enableUTF8Print();       //允许UTF8

  //远程通信数据
  mydata["status"] = 1;
  mydata["update"] = 0;
  serializeJson(mydata, jsondata);

  // ### 添加WiFi信息
  WiFiMulti.addAP("", "");

  pinMode(16, OUTPUT);  //用于连接Wi-Fi的LED指示灯
  pwm.begin(); //伺服电机启动
  pwm.setPWMFreq(60); //伺服电机以60hz刷新率工作
  delay(1000);

  //初始表情
  zc();
  normal();
  //开始WiFi连接
  startWiFiConnect();

  //开始MQTT连接
  client.setServer(mqtt_broker, mqtt_port);
  client.setCallback(callback);
  startMqttConnect();
}

//MQTT消息回调
void callback(char* topic, byte* payload, unsigned int length) {
  Serial.print("Message arrived: ");
  Serial.println(topic);
  Serial.print("Message:");
  for (int i = 0; i < length; i++) {
      Serial.print((char) payload[i]);
  }
  Serial.println();
  Serial.println("-----------------------");
  DynamicJsonDocument doc(1024);
  deserializeJson(doc, payload);
  JsonObject jsonObj = doc.as<JsonObject>();
  JsonObject params =jsonObj["params"];
  if (params["hello"] == 1){
    client.publish(t_topic, jsondata);
    normal();
    nh();
    hello();
